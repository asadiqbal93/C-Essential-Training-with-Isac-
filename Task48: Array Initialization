/*
        Alternative array initialization
        ________________________________

    - Stating the size
        int classrooms[5] = {15,18,10,23,15};
    -Without explicit size
        float heights [] = {5.5,6.1,5.8};

*/


#include <stdio.h>
#include <stdlib.h>

#define MAX_LETTERS 20

int main(void){
    int    classrooms[5]               ={15, 18, 10, 23, 15};
    float  heights []                ={5.5, 6.1, 5.8};
    char   lastName[MAX_LETTERS]      = "Mr.Iqbal";

    return EXIT_SUCCESS;
}
